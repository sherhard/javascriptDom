			<p>2</p>
			<p>20</p>




	<script type="text/javascript">
		
		let elems=document.getElementsByTagName('p');
			for(let i=0; i<elems.length; i++){
				elems[i].addEventListener('click',func);
			}
		function func(){
			this.innerHTML=this.innerHTML*this.innerHTML;
		
		}


</script>
		


