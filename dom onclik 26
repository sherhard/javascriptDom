		<p id="elem">поменять на h3</p>
		<input type="button" onclick="btn()" value="нажми на меня">

	<script type="text/javascript">

		function btn(){
			let elem=document.getElementById('elem');
			elem.outerHTML='<h3>поменял на h3 </h3>'
		}
		
		

	</script>
		


