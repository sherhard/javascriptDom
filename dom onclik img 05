	<img src="team/member02-k.jpg" alt="" id="in_text">


	<script type="text/javascript">

		function buttonClick(){
			let input =document.getElementById('input');
			let intex=document.getElementById('in_text');
			let img1=input.src;
			input.src=intex.src;
			intex.src=img1
			

		}
	</script>


